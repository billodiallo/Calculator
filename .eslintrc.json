{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "sourceType": "module",
            "allowImportExportEverywhere": true
        },
        "ecmaVersion": 12
    },
    "plugins": [
        "react"
        
    ],
    "rules": {
        "react/react-in-jsx-scope": "off"
    },
    "settings": {
       "react": {
         "version": "detect"
        }
      }
    
}
